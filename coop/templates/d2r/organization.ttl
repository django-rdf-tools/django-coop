map:organization_web a d2rq:PropertyBridge;
    d2rq:belongsToClassMap map:organization;
    d2rq:property foaf:homepage;
    d2rq:column "coop_local_organization.web";
    d2rq:condition "coop_local_organization.web <> ''";
